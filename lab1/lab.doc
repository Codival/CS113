\normalpage{ch1 Proposiational Calculus (Conjuntion)}{
Exploring compound propositions

\qi{Conjunction: $p \land q$}

\begin{tabular}{|c|c||c|}
\hline
$ p $ & $ q $ & $ (p \wedge q) $ \\
\hline
F & F & F \\
F & T & F \\
T & F & F \\
T & T & T \\
\hline
\end{tabular}
\label{table:tt1}

=SML
fun AND (false,x) = false | AND (true,x) = x;
val truthValues = [(false,false),(false,true),(true,false),(true,true)];
map AND truthValues;
